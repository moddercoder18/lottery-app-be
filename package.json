{
  "name": "lotteryapp",
  "version": "1.0.0",
  "description": "lotteryapp backend in Typescript Node & Mongo Rest API server featuring NestJS\n",
  "author": "",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist logs",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write src test --ignore-unknown",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --detectOpenHandles --config  ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nest-middlewares/serve-static": "6.0.0",
    "@nest-modules/mailer": "1.3.22",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "8.2.6",
    "@nestjs/core": "8.2.6",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/mongoose": "9.0.2",
    "@nestjs/passport": "8.1.0",
    "@nestjs/platform-express": "8.2.6",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "5.1.5",
    "aws-sdk": "^2.1290.0",
    "axios": "^1.4.0",
    "bcryptjs": "2.4.3",
    "bunyan": "1.8.15",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "compression": "1.7.4",
    "dotenv": "14.2.0",
    "form-data": "^4.0.0",
    "handlebars": "^4.7.7",
    "helmet": "5.0.2",
    "lodash": "4.17.21",
    "moment-timezone": "^0.5.43",
    "mongoose": "6.4.6",
    "morgan": "1.10.0",
    "multer": "^1.4.5-lts.1",
    "nest": "^0.1.6",
    "nest-morgan": "2.0.0",
    "nestjs": "^0.0.1",
    "node-tesseract-ocr": "^2.2.1",
    "node-text-from-image": "^1.0.1",
    "node-xlsx": "^0.21.0",
    "nodemailer": "6.7.2",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "paypal-rest-sdk": "^1.8.1",
    "reflect-metadata": "^0.1.13",
    "rotating-file-stream": "^1.4.3",
    "rxjs": "^7",
    "swagger-ui-express": "^4.0.7",
    "tesseract.js": "2.1.1",
    "twilio": "^4.11.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "8.2.6",
    "@types/bcryptjs": "2.4.2",
    "@types/bunyan": "1.8.8",
    "@types/compression": "1.7.2",
    "@types/cron": "^2.0.1",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.0",
    "@types/helmet": "4.0.0",
    "@types/inline-css": "^3.0.1",
    "@types/jest": "24.0.15",
    "@types/lodash": "4.14.178",
    "@types/mongodb": "4.0.7",
    "@types/mongoose": "5.11.97",
    "@types/morgan": "1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.18.16",
    "@types/nodemailer": "6.4.4",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "@types/paypal-rest-sdk": "^1.7.6",
    "@types/supertest": "2.0.8",
    "@types/uuid": "8.3.4",
    "@types/validator": "13.7.1",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "eslint": "8.7.0",
    "jest": "24.8.0",
    "mongoose-seed-2": "^0.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "10.4.0",
    "tsc-watch": "4.6.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "yarn@3.1.1"
}
